<Window x:Class="ReasArch.ImageFullScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ReasArch"
        mc:Ignorable="d"
        Title="ImageFullScreen" Height="600" Width="1000">
    <DockPanel Background="#757575">
        <Image Name="FullScreenImage" Source="{Binding sourceimage, Converter={StaticResource StringToImageConverter}}"/>
    </DockPanel>
</Window>
